<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.button {
			color: White;
			font-size: 14px;
			background-color: #cc3333;
 		 }
 		.label {
 			color: Silver;
		}
		.notice {
			padding: 4px 8px;
			font-weight: bold;
			background-color: #ffff99;
	    }
	    .background{
	    	background-color: papayawhip;
		}
	</ui:style>
	
	<g:AbsolutePanel width="1340px" height="100" ui:field="toolbar" styleName="{style.background}">
		<g:at left="28" top="22">
			<g:Button width="157px" height="36px" text="Request" styleName="{style.button}" ui:field="request"/>
		</g:at>
		<g:at left="230" top="30">
			<g:Label text="- This positon for Menubar or Toolbar" width="250px" height="16px" styleName="{style.label}" ui:field="defaulttitle"/>
		</g:at>
		<g:at left="655" top="0">
			<g:Label styleName="{style.notice}" ui:field="noticelabel"/>
		</g:at>
		<g:at left="230" top="52">
			<g:Button width="69px" height="28px" title="Mark Option" ui:field="markbutton">
				<img src="resource/markbutton.png" height="20px"/> 
			</g:Button>
		</g:at>
		<g:at left="321" top="52">
			<g:Button width="69px" height="28px" title="Back To List" ui:field="backbutton">
				<img src="resource/backbutton.png" height="20px"/> 
			</g:Button>
		</g:at>
		<g:at left="411" top="52">
			<g:Button width="69px" height="28px" title="Refresh" ui:field="refreshbutton">
				<img src="resource/refreshbutton.png" height="20px"/> 
			</g:Button>
		</g:at>
		<g:at left="502" top="52">
			<g:Button width="69px" height="28px" title="Print" ui:field="printbutton">
				<img src="resource/printbutton.png" height="21px"/> 
			</g:Button>
		</g:at>
		<g:at left="591" top="52">
			<g:Button width="69px" height="28px" title="Table Setting" ui:field="settingbutton">
				<img src="resource/settingbutton.png" height="20px"/> 
			</g:Button>
		</g:at>
		<g:at left="685" top="52">
			<g:Button width="69px" height="28px" title="Delete Request Forever" ui:field="deletebutton">
				<img src="resource/deletebutton.png" height="21px"/> 
			</g:Button>
		</g:at>
		<g:at left="1126" top="30">
			<g:Label ui:field="datelabel" text="Time Start" styleName="{style.label}"/>
		</g:at>
	</g:AbsolutePanel>

</ui:UiBinder> 