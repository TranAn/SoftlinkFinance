<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.label {
			font-size: 14px;
			font-weight: bold;
		}
		.button {
			color: White;
			background-color: #cc3333;
		}
		.none {
		}
		.label2 {
			color: Gray;
			font-size: 11px;
			font-weight: bold;
		}
		.panel {
	  	  padding: 10px;
	 	}
	 	.label3 {
			color: Red;
		}
		.textbox {
			color: Silver;
		}
	</ui:style>
 	
	<g:HTMLPanel width="720px" styleName='{style.panel}'>
	<div align='left'>
	<g:AbsolutePanel height="378px" width="720px">
		<g:at left="141" top="68">
			<g:Label text="Reporter:"/>
		</g:at>
		<g:at left="24" top="68">
			<g:Image url="resource/request.png" width="103px" height="85px"/>
		</g:at>
		<g:at left="141" top="137">
			<g:Label text="Manager:"/>
		</g:at>
		<g:at left="214" top="68">
			<g:Label text="test@gmail.com" width="125px" height="16px" styleName="{style.important}" ui:field="reporter"/>
		</g:at>
		<g:at left="141" top="102">
			<g:Label text="Requester:" ui:field="requesterlabel"/>
		</g:at>
		<g:at left="214" top="102">
			<g:TextBox width="121px" height="16px" text="requester" styleName="textbox" ui:field="requester"/>
		</g:at>
		<g:at left="214" top="137">
			<g:ListBox width="125px" height="20px" ui:field="manager"/>
		</g:at>
		<g:at left="82" top="180">
			<g:ListBox width="109px" height="22px" ui:field="account" styleName="{style.none}"/>
		</g:at>
		<g:at left="259" top="180">
			<g:ListBox width="80px" height="22px" ui:field="currency" styleName="{style.none}"/>
		</g:at>
		<g:at left="105" top="214">
			<g:IntegerBox width="70px" height="16px" styleName="textbox" text="0" ui:field="real_amount" alignment="RIGHT"/>
		</g:at>
		<g:at left="265" top="214">
			<g:IntegerBox width="70px" height="16px" text="0" styleName="textbox" ui:field="bill_amount" alignment="RIGHT"/>
		</g:at>
		<g:at left="24" top="220">
			<g:Label text="Real Amount:" width="89px" height="16px"/>
		</g:at>
		<g:at left="185" top="220">
			<g:Label text="Bill Amount:"/>
		</g:at>
		<g:at left="24" top="180">
			<g:Label text="Account:"/>
		</g:at>
		<g:at left="197" top="180">
			<g:Label text="Currency:"/>
		</g:at>
		<g:at left="389" top="68">
			<g:Label text="Relates Infomation:" styleName="{style.label}"/>
		</g:at>
		<g:at left="389" top="107">
			<g:Label text="Customer ID:"/>
		</g:at>
		<g:at left="233" top="314">
			<g:Label text="- An Email and Request will be sent to Manager" width="293px" height="16px" styleName="{style.label2}"/>
		</g:at>
		<g:at left="359" top="56">
			<g:Image url="resource/line1.png" width="12px" height="236px"/>
		</g:at>
		<g:at left="24" top="292">
			<g:Image width="678px" height="16px" url="resource/line2.png"/>
		</g:at>
		<g:at left="105" top="253">
			<g:TextArea width="228px" height="27px" styleName="textbox" text="request for?" ui:field="description"/>
		</g:at>
		<g:at left="468" top="101">
			<g:ListBox width="76px" height="22px" ui:field="cus_id" styleName="{style.none}"/>
		</g:at>
		<g:at left="549" top="108">
			<g:Label text="Assets_id:"/>
		</g:at>
		<g:at left="617" top="104">
			<g:ListBox width="85px" height="20px" ui:field="assets_id" styleName="{style.none}"/>
		</g:at>
		<g:at left="402" top="261">
			<g:Label text="Document:"/>
		</g:at>
		<g:at left="403" top="203">
			<g:Label text="Reference:"/>
		</g:at>
		<g:at left="433" top="148">
			<g:Label text="Tags:" ui:field="tagslabel"/>
		</g:at>
		<g:at left="670" top="68">
			<g:Image ui:field="refresh" url="resource/refresh2.PNG" width="32px" height="28px" title="refresh" visible="false"/>
		</g:at>
		<g:at left="670" top="68">
			<g:Image url="resource/refresh1.PNG" width="32px" height="28px" ui:field="image"/>
		</g:at>
		<g:at left="30" top="261">
			<g:Label text="Description:" ui:field="descriptionlabel"/>
		</g:at>
		<g:at left="468" top="253">
			<g:TextArea width="228px" height="27px" ui:field="document" styleName="textbox" text="relate with documents?"/>
		</g:at>
		<g:at left="468" top="203">
			<g:TextArea width="228px" height="27px" styleName="textbox" text="relate with someone?" ui:field="reference"/>
		</g:at>
		<g:at left="468" top="148">
			<g:TextArea styleName="textbox" text="relate with project?" ui:field="tags" width="228px" height="30px"/>
		</g:at>
		<g:at left="192" top="336">
			<g:Button width="115px" height="28px" text="Send Request" styleName="{style.button}" ui:field="sendrequest"/>
		</g:at>
		<g:at left="334" top="336">
			<g:Button width="80px" height="28px" styleName="{style.none}" text="Draft" ui:field="draft"/>
		</g:at>
		<g:at left="442" top="336">
			<g:Button width="102px" height="28px" text="Close" styleName="{style.none}" ui:field="closebutton"/>
		</g:at>
		<g:at left="24" top="27">
			<g:Button width="678px" height="22px" text="income-request" styleName="incomebtn" ui:field="btnFinance"/>
		</g:at>
		<g:at left="282" top="10">
			<g:Label width="169px" height="16px" styleName="{style.label2}" text="Click to change request type"/>
		</g:at>
	</g:AbsolutePanel>
	</div>
	</g:HTMLPanel>
</ui:UiBinder> 